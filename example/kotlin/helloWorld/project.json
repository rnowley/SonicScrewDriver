{
    "Name": "HelloWorld",
    "Version": "0.0.1",
    "Description": "A demo project for testing compiling Kotlin.",
    "Language": "kotlin",
	"Destination": "./build/",
    "SourceFiles": [
        "main/hello.kt"
    ],
    "ClassPath": [
        "./build/"
     ],
    "RunArguments": [
    	"Raymond"
    ],
    "BuildTarget": "executable",
    "TestProject": {
        "SourceFiles": [
            "test/greeterTest.kt"
        ],
        "DestinationDirectory": "./build/",
        "TestFramework": "junit",
        "MainClass": "org.junit.runner.JUnitCore",
        "ClassPath": [
            "./lib/junit-4.12.jar",
            "./lib/hamcrest-core-1.3.jar",
			"./lib/kotlin-test-1.0.0.jar",
			"./lib/kotlin-test-junit-1.0.0.jar",
            "./build/out.jar"
        ],
        "RunArguments": [
            "GreeterTest"
        ]
    }
}
