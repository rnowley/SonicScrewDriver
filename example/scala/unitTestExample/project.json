{
    "name": "HelloWorld",
    "version": "0.0.1",
    "Description": "A demo project for testing compiling to scala.",
    "Language": "scala",
	"sourceDirectory": "./src/main/scala/",
    "sourceFiles": [
        "org/me/hotel/Guest.scala",
		"org/me/hotel/Hotel.scala",
		"org/me/hotel/Room.scala"
    ],
	"ClassPath": [
		"./build/"
	 ],
    "DestinationDirectory": "./build/",
	"TestProject": {
		"SourceFiles": [
			"org/me/hotel/GuestTest.scala",
			"org/me/hotel/HotelTest.scala",
			"org/me/hotel/MySuites.scala",
			"org/me/hotel/RoomTest.scala",
			"org/me/hotel/UnitTest.scala"
		],
		"DestinationDirectory": "./build/",
		"SourceDirectory": "./src/test/scala/",
		"MainClass": "org.scalatest.tools.Runner",
		"ClassPath": [
			"./lib/scalatest_2.11-2.2.6.jar"
		],
		"RunArguments": [
			"-o",
			"-R",
			"./build/"
		]
	}
}
