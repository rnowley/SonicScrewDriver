{
	"name": "HelloWorldJava",
	"Version": "0.0.1",
	"Description": "A demo project for testing compiling Java.",
	"Language": "java",
	"SourceDirectory": "./src/",
	"SourceFiles": [
		"main/ExampleProgram.java",
		"main/Greeter.java"
	],
	"MainClass": "ExampleProgram",
	"ClassPath": [
		"./build/"
	 ],
	"SourceVersion": "1.8",
	"LintWarnings": [
		"all"
	],
	"TestProject": {
		"SourceFiles": [
			"test/GreeterTest.java"
		],
		"DestinationDirectory": "./build/",
		"SourceDirectory": "./src/",
		"TestFramework": "junit",
		"MainClass": "org.junit.runner.JUnitCore",
		"ClassPath": [
			"./lib/junit-4.12.jar",
			"./lib/hamcrest-core-1.3.jar",
			"./build/"
		],
		"RunArguments": [
			"GreeterTest"
		]
	},
	"DocumentationProject": {
		"author": true,
		"ClassPath": [

		],
		"DestinationDirectory": "./doc/html",
		"LinkSource": true
	}
}
